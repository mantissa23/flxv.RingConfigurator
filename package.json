{
  "name": "flxv.constructor",
  "version": "1.0.4",
  "private": true,
  "description": "Конструктор моделей от feliksov.ru",
  "author": "Dmitriy Maziuk",
  "scripts": {
    "test": "node ./node_modules/jest-cli/bin/jest",
    "test:upd-snapshots": "npm t -- -u",
    "test:watch": "npm t -- --watch",
    "test:coverage": "npm t -- --coverage",
    "clean-build": "rimraf .nuxt dist .eslintcache",
    "build": "nuxt build",
    "analyze": "nuxt build --analyze",
    "lint": "eslint --cache --ext .js,.vue .",
    "lint:fix": "eslint --cache --fix --ext .js,.vue .",
    "dev": "npm run clean-build && nuxt",
    "generate": "nuxt generate",
    "pretty": "prettier --loglevel warn --write \"*.{js,vue}\" --ignore-path .eslintignore",
    "start": "nuxt start",
    "npm:clean": "npm cache clean --force && rimraf package-lock.json node_modules",
    "npm:update": "ncu -u --packageFile package.json --loglevel verbose"
  },
